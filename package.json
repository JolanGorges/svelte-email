{
	"name": "svelte-email",
	"version": "0.0.4",
	"description": "Build emails with Svelte",
	"author": {
		"name": "Carsten Lebek"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"docs:build": "svelte-kit sync && vite build",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@iconify-json/ri": "^1.1.19",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.3",
		"@sveltejs/package": "^1.0.2",
		"@svelteness/kit-docs": "^1.1.5",
		"@types/gtag.js": "^0.0.12",
		"@types/html-to-text": "^9.0.4",
		"@types/pretty": "^2.0.3",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.17",
		"clsx": "^1.2.1",
		"csstype": "^3.1.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"i": "^0.3.7",
		"npm": "^9.9.2",
		"postcss": "^8.4.33",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"shiki": "^0.14.7",
		"svelte-check": "^3.6.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"unplugin-icons": "^0.15.3",
		"vite": "^4.5.2"
	},
	"type": "module",
	"dependencies": {
		"html-to-text": "^9.0.5",
		"pretty": "^2.0.0",
		"svelte": "^3.59.2"
	},
	"exports": {
		"./package.json": "./package.json",
		"./components/Body.svelte": "./components/Body.svelte",
		"./components/Button.svelte": "./components/Button.svelte",
		"./components/Column.svelte": "./components/Column.svelte",
		"./components/Container.svelte": "./components/Container.svelte",
		"./components/Head.svelte": "./components/Head.svelte",
		"./components/Heading.svelte": "./components/Heading.svelte",
		"./components/Hr.svelte": "./components/Hr.svelte",
		"./components/Html.svelte": "./components/Html.svelte",
		"./components/Img.svelte": "./components/Img.svelte",
		"./components/Link.svelte": "./components/Link.svelte",
		"./components/Preview.svelte": "./components/Preview.svelte",
		"./components/Section.svelte": "./components/Section.svelte",
		"./components/Text.svelte": "./components/Text.svelte",
		".": {
			"svelte": "./index.js",
			"types": "./index.d.ts"
		},
		"./render": "./render.js",
		"./utils": "./utils.js"
	},
	"svelte": "./index.js",
	"types": "./index.d.ts"
}
